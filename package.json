{
  "name": "remox",
  "version": "0.1.0",
  "description": "React & MobX boilerplate",
  "main": "src/index.js",
  "scripts": {
    "lint": "xo",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "xo && react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
		"extends": "xo"
  },
  "xo": {
    "esnext": true,
    "semicolon": false,
    "space": 2
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimzhan/remox.git"
  },
  "keywords": [
    "react",
    "mobx",
    "es7",
    "es6",
    "boilerplate",
    "skeleton"
  ],
  "author": "Jim Zhan <jim.zhan@me.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jimzhan/remox/issues"
  },
  "homepage": "https://github.com/jimzhan/remox#readme",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "^1.0.17"
  },
  "devDependencies": {
    "eslint-config-xo": "^0.19.0",
    "eslint-config-xo-react": "^0.14.0",
    "xo": "^0.18.2"
  }
}
